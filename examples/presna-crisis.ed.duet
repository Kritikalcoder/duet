-- Presna algorithm
let getXY = sλ m : 𝕄 [L∞ , U | ★ , ℕ ∷ ℕ ∷ ℕ ∷ ℕ ∷ 𝕊 ∷ ℕ ∷ ℕ ∷ ℕ ∷ ℕ ∷ 𝕊 ∷ [] ] ⇒
  ⟨ row#[0,2], row#[0,3] ⟩
in
let main = pλ ε : ℝ⁺,
              δ : ℝ⁺
              .
              -- interval:ℕ,phone_id:ℕ,x:ℕ,y:ℕ,hash:𝕊
              incoming : 𝕄 [L∞ , U | ★ , ℕ ∷ ℕ ∷ ℕ ∷ ℕ ∷ 𝕊 ∷ [] ],
              outgoing : 𝕄 [L∞ , U | ★ , ℕ ∷ ℕ ∷ ℕ ∷ ℕ ∷ 𝕊 ∷ [] ],
              blocks   : set ℕ × ℕ,      -- set of all the blocks of interest
              crisis   : set ℕ × ℕ,      -- set of blocks in crisis
              ε : ℝ⁺[ε],
              δ : ℝ⁺[δ]
              ⇒
  -- CRISIS mode
  -- result of the join is a ℕ ∷ ℕ ∷ ℕ ∷ ℕ ∷ 𝕊 ∷ ℕ ∷ ℕ ∷ ℕ ∷ ℕ ∷ 𝕊 ∷ []
  --                        |    INCOMING      |      OUTGOING     |

  parallel (join₁[outgoing, 4, incoming, 4]) { true, false } { row ⇒ (getXY row) ∈ crisis }
      { n, pb ⇒ 
          r ← parallel pb blocks { row ⇒ getXY row }
                { b, p ⇒ noisyCount ← gauss[ℝ⁺[1.0], ε, δ] <incoming, outgoing> { rows p };
                         return ⟨b, noisyCount⟩ };
          return ⟨n, r⟩
      }
in main
